# Environment configuration example for mcp-dashboard
# Copy this file to .env and fill in the values

NODE_ENV=development

# Parameters
NEXT_PUBLIC_API_URL=your_value_here (required) # default: http://localhost:3000/api
NEXT_PUBLIC_WS_URL=your_value_here # default: ws://localhost:3000
NEXT_PUBLIC_AUTH_ENABLED=your_value_here # default: false
NEXT_PUBLIC_ANALYTICS_ID=your_value_here # default: dummy-analytics-id
API_TIMEOUT=your_value_here # default: 30000
REFRESH_INTERVAL=your_value_here # default: 5000

# Secrets
NEXTAUTH_SECRET=your_secret_here
NEXTAUTH_URL=your_secret_here
