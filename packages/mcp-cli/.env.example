# Environment configuration example for mcp-cli
# Copy this file to .env and fill in the values

NODE_ENV=development

# Parameters
MCP_SERVER_URL=your_value_here (required) # default: http://localhost:3000
MCP_API_PREFIX=your_value_here # default: api
CLI_CONFIG_PATH=your_value_here # default: ~/.mcp/config.json
CLI_LOG_LEVEL=your_value_here # default: info
DEFAULT_TIMEOUT=your_value_here # default: 30000

# Secrets
CLI_API_KEY=your_secret_here
